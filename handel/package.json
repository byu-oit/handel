    "common-tags": "^1.7.2",
    "crypto-random-string": "^1.0.0",
    "fs-extra": "^5.0.0",
    "handel-extension-api": "^0.40.0",
    "handel-extension-support": "^0.40.0",
    "inquirer": "^3.3.0",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.15",
    "ncp": "^2.0.0",
    "pjson": "^1.0.9",
    "tmp": "0.0.33",
    "uuid": "^3.0.1",
    "winston": "2.3.1"
