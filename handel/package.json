    "@types/fs-extra": "^5.1.0",
    "@types/inquirer": "0.0.36",
    "@types/js-yaml": "^3.12.1",
    "@types/lodash": "^4.14.136",
    "@types/minimist": "^1.2.0",
    "@types/mocha": "^2.2.44",
    "@types/ncp": "^2.0.1",
    "@types/node": "^8.10.51",
    "@types/sinon": "^2.3.7",
    "@types/sinon-chai": "^2.7.35",
